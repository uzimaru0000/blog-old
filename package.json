{
  "name": "blog",
  "version": "1.0.0",
  "description": "",
  "bin": {
    "blog": "./bin/run.js"
  },
  "scripts": {
    "start": "vercel dev -d",
    "test:api": "ava api/test/*.test.ts",
    "watch": "concurrently \"npm run watch:server\" \"npm run watch:client\" \"npm run watch:api\"",
    "watch:server": "parcel watch -t node --bundle-node-modules -d ./main/dist ./main/src/server/index.tsx",
    "watch:client": "parcel watch -d ./main/dist/js ./main/src/client/index.tsx",
    "watch:api": "parcel watch -t node --bundle-node-modules -d ./api/dist ./api/src/index.ts",
    "watch:cli": "parcel watch -t node -d ./cli/dist ./cli/src/index.ts",
    "build:server": "parcel build -t node --bundle-node-modules --no-source-maps -d ./main/dist ./main/src/server/index.tsx",
    "build:client": "parcel build -d ./main/dist/js --no-source-maps ./main/src/client/index.tsx",
    "build:api": "parcel build -t node --bundle-node-modules --no-source-maps -d ./api/dist ./api/src/index.ts",
    "build:cli": "parcel build -t node --no-source-maps -d ./cli/dist ./cli/src/index.ts",
    "build": "concurrently \"npm run build:server\"  \"npm run build:client\" \"npm run build:api\"",
    "deploy": "./bin/deploy.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/micro": "^7.3.3",
    "@types/micro-cors": "^0.1.0",
    "@types/microrouter": "^3.1.1",
    "@types/node-emoji": "^1.8.1",
    "@types/node-fetch": "^2.5.4",
    "@types/prompts": "^2.0.3",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-syntax-highlighter": "^11.0.4",
    "@types/request-promise": "^4.1.45",
    "@types/sharp": "^0.23.1",
    "@types/styled-components": "^4.4.1",
    "@types/test-listen": "^1.1.0",
    "ava": "^2.4.0",
    "concurrently": "^5.0.2",
    "node-elm-compiler": "^5.0.4",
    "parcel": "^1.12.4",
    "request-promise": "^4.2.5",
    "sass": "^1.24.2",
    "test-listen": "^1.1.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.4",
    "vercel": "^19.1.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@mojotech/json-type-validation": "^3.1.0",
    "commander": "^4.0.1",
    "faunadb": "^2.10.0",
    "form-data": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "micro": "^9.3.4",
    "micro-compress": "^1.0.0",
    "micro-cors": "^0.1.1",
    "microrouter": "^3.1.3",
    "node-emoji": "^1.10.0",
    "node-fetch": "^2.6.0",
    "prompts": "^2.3.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-markdown": "^4.3.0",
    "react-router-dom": "^5.1.2",
    "react-syntax-highlighter": "^9.0.1",
    "sharp": "^0.23.4",
    "styled-components": "^4.4.1",
    "styled-media-query": "^2.1.2",
    "xhr2": "^0.2.0"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
