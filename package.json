    "start": "now dev -d",
    "watch:server": "parcel watch -t node --bundle-node-modules -d ./main/dist ./main/src/server/index.tsx",
    "watch:client": "parcel watch -d ./main/dist/js ./main/src/client/index.tsx",
    "watch:api": "parcel watch -t node --bundle-node-modules -d ./api/dist ./api/src/index.ts",
    "build:server": "parcel build -t node --bundle-node-modules --no-source-maps -d ./main/dist ./main/src/server/index.tsx",
    "build:client": "parcel build -d ./main/dist/js --no-source-maps ./main/src/client/index.tsx",
    "build:api": "parcel build -t node --bundle-node-modules --no-source-maps -d ./api/dist ./api/src/index.ts",
    "build:cli": "parcel build -t node --no-source-maps -d ./cli/dist ./cli/src/index.ts",
    "build": "npm run build:server && npm run build:client && npm run build:api",
    "deploy": "./bin/deploy.sh"
  },
