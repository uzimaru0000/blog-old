{
  "compilerOptions": {
    "target": "es5",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "lib": ["dom", "ES2017"],
    "jsx": "react",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
