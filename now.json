{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "dist/favicon.ico"
    },
    {
      "src": "/js/(.*)",
      "dest": "dist/js/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "dist/css/$1"
    },
    {
      "src": "/img/(.*)",
      "dest": "dist/img/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.html"
    }
  ]
}
